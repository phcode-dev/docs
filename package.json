{
  "name": "docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "npm run buildAPIDocs && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "buildAPIDocs": "shx rm -rf phoenix && shx rm -rf api/API-Reference && git clone --depth 1 --filter=blob:none --sparse https://github.com/phcode-dev/phoenix.git && cd phoenix && git sparse-checkout set docs/API-Reference && cd .. && shx cp -r phoenix/docs/API-Reference api/API-Reference && shx rm -rf phoenix"
  },
  "dependencies": {
    "@docusaurus/core": "3.5.2",
    "@docusaurus/plugin-google-gtag": "3.5.2",
    "@docusaurus/plugin-sitemap": "3.5.2",
    "@docusaurus/preset-classic": "3.5.2",
    "@docusaurus/theme-live-codeblock": "3.5.2",
    "@docusaurus/theme-mermaid": "3.5.2",
    "@gracefullight/docusaurus-plugin-microsoft-clarity": "^1.0.0",
    "@mdx-js/react": "3.0.1",
    "clsx": "2.1.1",
    "prism-react-renderer": "2.4.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rehype-katex": "7.0.1",
    "remark-math": "6.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.5.2",
    "@docusaurus/types": "3.5.2",
    "shx": "^0.3.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
